   "outputs": [],
   "source": [
    "managers = nx.read_gml(\"managers.gml\")\n",
    "reports = nx.read_gml('reports.gml')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "aviv_tree = nx.dfs_tree(reports, 'albionb', depth_limit=3)\n",
    "nx.set_node_attributes(aviv_tree, \"box\", \"shape\")\n",
    "nx.nx_pydot.write_dot(aviv_tree, \"aviv.gv\")"
   ]
  }
